<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ouch.ouchworkout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/my_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WorkoutAct">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExecutingNextExerciseAct"
            android:noHistory="true"
            android:parentActivityName=".WorkoutAct">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WorkoutAct" />
        </activity>
        <activity
            android:name=".SettingsAct"
            android:parentActivityName=".WorkoutAct">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WorkoutAct" />
        </activity>
        <activity
            android:name=".ExerciseSelectionAct"
            android:noHistory="true" />
        <activity
            android:name=".ReviewerAct"
            android:noHistory="true" />
    </application>
</manifest>